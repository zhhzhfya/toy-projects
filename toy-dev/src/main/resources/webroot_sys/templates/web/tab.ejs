<div id="<%=modelCode%>tab" title="<%=title%>" data-options="closable:<%=closable%>" style="height:inherit;">
	<div class="easyui-layout" style="padding:10px;" data-options="fit:'true'">
		<div data-options="region:'west',split:true" style="width:200px;padding:2px">
			<ul id="tt" class="easyui-tree" data-options="url:'/plugin/easyui/tree_data1.json',method:'get',animate:true"></ul>
		</div>
		<div data-options="region:'center'" style="padding:2px">
			<table class="easyui-datagrid"
					data-options="url:'/plugin/easyui/datagrid_data1.json',method:'get',border:false,singleSelect:true,fit:true,fitColumns:true">
				<thead>
					<tr>
						<th data-options="field:'itemid'" width="80">Item ID</th>
						<th data-options="field:'productid'" width="100">Product ID</th>
						<th data-options="field:'listprice',align:'right'" width="80">List Price</th>
						<th data-options="field:'unitcost',align:'right'" width="80">Unit Cost</th>
						<th data-options="field:'attr'" width="150">Attribute</th>
						<th data-options="field:'status',align:'center'" width="50">Status</th>
					</tr>
				</thead>
			</table>
		</div>
	</div>
</div>